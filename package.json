{
  "name": "expandable-ts-frontend",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./public/main.js",
    "build": "tsc",
    "dev": "nodemon --watch 'src/**' --ext 'ts,json' --exec 'ts-node -r tsconfig-paths/register ./src/app.ts' --ignore 'node_modules'",
    "format": "prettier --write .",
    "lint": "eslint . ",
    "lint:fix": "eslint . --fix "
  },
  "dependencies": {
    "htmx.org": "^1.9.2",
    "nodemon": "^3.1.9"
  },
  "devDependencies": {
    "typescript": "^5.7.2"
  }
}
